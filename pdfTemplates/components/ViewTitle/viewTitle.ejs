<div
  style="
    display: flex;
    place-content: flex-start;
    place-items: center;
    gap: 10px;
    height: max-content;
  "
  ,
>
  <div
    style="
      display: flex;
      place-content: center;
      place-items: center;
      height: 44px;
      width: 44px;
      border-radius: 50%;
      font-weight: bold;
      font-size: 14px;
      background-color: <%= color || '#000000' %>;
    "
  >
    <%= symbol %>
  </div>

  <p style="font-weight: 700; font-family: Mulish, sans-serif; letter-spacing: 3px;">
    <% let result; switch (symbol) { case 'C/C': result = 'CHIEF COMPLAINTS';break; 
    case 'O/E': result = 'EXAMINATION'; break; 
    case 'I': result ='INVESTIGATION'; break; 
    case 'Rx': result ='TREATMENT'; break;
    case 'TDx': result = 'DIAGNOSIS AND PROGNOSIS';  break;
    case 'F/U': result = 'FOLLOW UPS';  break;
    case 'A': result = 'ADVICE';  break;
     default: result = ''; break;
    } %> <%= result %>
  </p>
</div>
